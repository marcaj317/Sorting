#include <stdio.h>
#include <iostream>
using namespace std;

int main(void){
	double x[ ] = {2.5, 1.5, 4.0, 7.0, 3.5, 6.5, 8.5, 1.0}; 
	int i, j, k, num;
	for(i = 0; i <= sizeof(x) - 1; i++){
		k = i;
		for(j = i + 1; j <= sizeof(x) - 1; j++){
			if(x[j] < x[k])
				k = j;
			num = x[k];
			x[k] = x[i];
			x[j] = num;
		}
		printf("%f \n", x[i]);
	}
	
	system("pause");
	return 0;
}
